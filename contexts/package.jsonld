{
    "@context": {
        "@version": 1.1,
        "doap": "http://usefulinc.com/ns/doap#",
        "foaf": "http://xmlns.com/foaf/0.1/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "npm": "https://linkedsoftwaredependencies.org/vocabularies/npm#",
        "name": "doap:name",
        "Package": "doap:Project",
        "Person": "foaf:Person",
        "description": "doap:description",
        "created": {
            "@id": "doap:created",
            "@type": "xsd:date"
        },
        "executable": "npm:main",
        "issue-tracker": "doap:bug-database",
        "repository": "doap:GitRepository",
        "browse": "doap:browse",
        "location": "doap:location",
        "programming-language": "doap:programming-language",
        "release": "doap:Version",
        "revision": "doap:revision",
        "download": "doap:download-page",
        "developer": {
            "@id": "foaf:Person",
            "@type": "@id"
        },
        "homepage": "doap:homepage",
        "license": "doap:license"
    }
}
